div(class="gameList basicContainer col-lg-12 col-md-12 col-sm-12")
  div(class="header rowFlexContainer row")
    h3(data-i18n="historicTabTitle" class="col-lg-12 col-md-12 col-sm-12")
  div(class="body row")
    div(class="col-lg-12 col-md-12 col-sm-12")
      table(class="col-lg-12 col-md-12 col-sm-12")
        thead
          tr
            th(data-i18n="historicServerName" class="col-lg-2 col-md-2 col-sm-2")
            th(data-i18n="historicServerAnimator" class="col-lg-2 col-md-2 col-sm-2")
            th(data-i18n="historicServerDate" class="col-lg-2 col-md-2 col-sm-2")
            th(data-i18n="historicServerQuestion" class="col-lg-6 col-md-6 col-sm-6")
        tbody
  div(class="formBottomStyle bottom row")
    div(style="visibility: hidden" class="rowFlexContainer col-lg-offset-1 col-lg-2 col-md-offset-1 col-md-2 col-sm-offset-1 col-sm-2")
      button(name="comeBack" data-i18n="comeBack" class="myCustomButton col-lg-12 col-md-12 col-sm-12")
    div(class="rowFlexContainer col-lg-offset-5 col-lg-4 col-md-offset-5 col-md-4 col-sm-offset-5 col-sm-4")
      button(name="deleteEntry" data-i18n="delete" class="myCustomButton col-lg-4 col-md-4 col-sm-4")
      button(name="open" data-i18n="historicOpen" class="myCustomButton col-lg-offset-1 col-lg-4 col-md-offset-1 col-md-4 col-sm-offset-1 col-sm-4")
