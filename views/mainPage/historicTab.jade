div#historicTab(class="formWrapperStyle rowFlexContainer")
  div(class="tabContent active col-lg-12 col-md-12 col-sm-12")
    div(class="header rowFlexContainer ow")
      h3(data-i18n="historicTabTitle" class="col-lg-12 col-md-12 col-sm-12")
    div(class="body row")
      div(class="col-lg-12 col-md-12 col-sm-12")
        table#myHistoric(class="col-lg-12 col-md-12 col-sm-12")
          thead
            tr
              th(data-i18n="historicServerName" class="col-lg-2 col-md-2 col-sm-2")
              th(data-i18n="historicServerAnimator" class="col-lg-2 col-md-2 col-sm-2")
              th(data-i18n="historicServerDate" class="col-lg-2 col-md-2 col-sm-2")
              th(data-i18n="historicServerQuestion" class="col-lg-6 col-md-6 col-sm-6")
          tbody
    div(class="formBottomStyle bottom row")
      div(class="rowFlexContainer col-lg-offset-8 col-lg-4 col-md-offset-8 col-md-4 col-sm-offset-8 col-sm-4")
        button#deleteEntry(data-i18n="historicDelete" class="myCustomButton col-lg-offset-1 col-lg-4 col-md-offset-1 col-md-4 col-sm-offset-1 col-sm-4")
        button#open(data-i18n="historicOpen" class="switch myCustomButton col-lg-offset-1 col-lg-4 col-md-offset-1 col-md-4 col-sm-offset-1 col-sm-4")

  div(class="partySheet col-lg-12 col-md-12 col-sm-12")
    div(class="header rowFlexContainer row")
      h3(data-i18n="partySheetTitle" class="col-lg-12 col-md-12 col-sm-12")
    div(class="body row")
      div(class="row")
        div(class="col-lg-5 col-md-5 col-sm-5")
          div(class="centeredItems row")
            label(data-i18n="partySheetServerField" class="col-lg-2 col-md-2 col-sm-2")
            input#partyName(readonly class="myCustomInput col-lg-offset-1 col-lg-6 col-md-offset-1 col-md-6 col-sm-offset-1 col-sm-6")
            input#partyDate
          div(class="centeredItems row")
            label(data-i18n="partySheetAnimatorField" class="col-lg-2 col-md-2 col-sm-2")
            input#partyAnimator(readonly class="myCustomInput col-lg-offset-1 col-lg-6 col-md-offset-1 col-md-6 col-sm-offset-1 col-sm-6")
        div(class="centeredItems col-lg-7 col-md-7 col-sm-7")
          label(data-i18n="partySheetQuestionField" class="col-lg-3 col-md-3 col-sm-3")
          input#partyQuestion(readonly class="myCustomInput col-lg-8 col-md-8 col-sm-8")
      div(class="rowFlexContainer row")
        div#productionViewer(class="imageBackground col-lg-7 col-md-7 col-sm-7")    
        div(class="col-lg-5 col-md-5 col-sm-5")
          div(class="row")
            table#partyPlayers(class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10")
              thead
                tr
                  th(data-i18n="partySheetPlayersField")
              tbody
    div(class="formBottomStyle bottom row")
      div(class="centeredItems col-lg-offset-1 col-lg-2 col-md-offset-1 col-md-2 col-sm-offset-1 col-sm-2")
        button#editProduction(data-i18n="partySheetEditProduction" class="myCustomButton col-lg-10 col-md-10 col-sm-10")
      div(class="rowFlexContainer col-lg-offset-4 col-lg-4 col-md-offset-4 col-md-4 col-sm-offset-4 col-sm-4")
        button#download(data-i18n="partySheetDownloadProduction" class="myCustomButton col-lg-offset-1 col-lg-5 col-md-offset-1 col-md-5 col-sm-offset-1 col-sm-5")
        button#close(data-i18n="partySheetClose" class="myCustomButton col-lg-offset-1 col-lg-5 col-md-offset-1 col-md-5 col-sm-offset-1 col-sm-5")
  
  div(class="productionEditor col-lg-12 col-md-12 col-sm-12")
    div(class="header rowFlexContainer row")
      h3(data-i18n="productionEditorTitle" class="col-lg-12 col-md-12 col-sm-12")
    div(class="body row")
      div#colorMenu(class="basicContainer stretchedImage menu")
        button(value="red" class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
        button(value="green" class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
        button(value="yellow" class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
        button(value="blue" class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
        button(value="white" class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
        button(value="purple" class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
        button(value="brown" class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
      div#svgMenu(class="basicContainer menu")
        div
          button#bouton(value="red" class="playerOnly col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
          button#color(value="red" class="playerOnly col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
          button#moveElement(class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
          button#centerSVG(class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
          button#legend(value="visible" class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1")
        div(class="centeredItems")
          label#zoomLevel(class="myCustomIndicator rowFlexContainer col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10 col-xs-offset-1 col-xs-1") Zoom x1.00
      div#productionEditor(class="basicContainer imageBackground")
        include contextualMenu.jade
        div#overlay
          div#overlayContent
            h4(class="legend") this legend will fill itself dynamically, don't forget you can resize it and move it.
    div(class="formBottomStyle bottom row")
      div(class="rowFlexContainer col-lg-offset-7 col-lg-4 col-md-offset-7 col-md-4 col-sm-offset-7 col-sm-4")
        button#saveProduction(data-i18n="productionEditorSave" class="switch myCustomButton col-lg-offset-1 col-lg-5 col-md-offset-1 col-md-5 col-sm-offset-1 col-sm-5")
        button#closeEditor(data-i18n="productionEditorClose" class="myCustomButton col-lg-offset-1 col-lg-5 col-md-offset-1 col-md-5 col-sm-offset-1 col-sm-5")
span(id="ruler")
