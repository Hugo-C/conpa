div#historicTab(class="formWrapperStyle rowFlexContainer")
  div(class="tabContent active col-lg-12 col-md-12 col-sm-12")
    div(class="header rowFlexContainer ow")
      h3(class="col-lg-12 col-md-12 col-sm-12") My historic
    div(class="body row")
      div(class="col-lg-12 col-md-12 col-sm-12")
        table#myHistoric(class="col-lg-12 col-md-12 col-sm-12")
          thead
            tr
              th(class="col-lg-2 col-md-2 col-sm-2") Server
              th(class="col-lg-2 col-md-2 col-sm-2") Animator
              th(class="col-lg-2 col-md-2 col-sm-2") Date
              th(class="col-lg-6 col-md-6 col-sm-6") Question
          tbody
    div(class="formBottomStyle bottom row")
      div(class="rowFlexContainer col-lg-offset-8 col-lg-4 col-md-offset-8 col-md-4 col-sm-offset-8 col-sm-4")
        button#deleteEntry(class="myCustomButton col-lg-offset-1 col-lg-4 col-md-offset-1 col-md-4 col-sm-offset-1 col-sm-4") Delete
        button#open(class="switch myCustomButton col-lg-offset-1 col-lg-4 col-md-offset-1 col-md-4 col-sm-offset-1 col-sm-4") Open

  div(class="partySheet col-lg-12 col-md-12 col-sm-12")
    div(class="header rowFlexContainer row")
      h3(class="col-lg-12 col-md-12 col-sm-12") My Party
    div(class="body row")
      div(class="row")
        div(class="col-lg-5 col-md-5 col-sm-5")
          div(class="centeredItems row")
            label(class="col-lg-2 col-md-2 col-sm-2") Server
            input#partyName(readonly class="myCustomInput col-lg-offset-1 col-lg-6 col-md-offset-1 col-md-6 col-sm-offset-1 col-sm-6")
            input#partyDate
          div(class="centeredItems row")
            label(class="col-lg-2 col-md-2 col-sm-2") Animator
            input#partyAnimator(readonly class="myCustomInput col-lg-offset-1 col-lg-6 col-md-offset-1 col-md-6 col-sm-offset-1 col-sm-6")
        div(class="centeredItems col-lg-7 col-md-7 col-sm-7")
          label(class="col-lg-3 col-md-3 col-sm-3") My Question
          input#partyQuestion(readonly class="myCustomInput col-lg-8 col-md-8 col-sm-8")
      div(class="rowFlexContainer row")
        div#myProduction(class="col-lg-7 col-md-7 col-sm-7")
          img(class="col-lg-12 col-md-12 col-sm-12")
        div(class="col-lg-5 col-md-5 col-sm-5")
          div(class="row")
            table#partyPlayers(class="col-lg-offset-1 col-lg-10 col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10")
              thead
                tr
                  th Players
              tbody
    div(class="formBottomStyle bottom row")
      div(class="rowFlexContainer col-lg-offset-7 col-lg-4 col-md-offset-7 col-md-4 col-sm-offset-7 col-sm-4")
        button#download(class="switch myCustomButton col-lg-offset-1 col-lg-5 col-md-offset-1 col-md-5 col-sm-offset-1 col-sm-5") Download
        button#close(class="myCustomButton col-lg-offset-1 col-lg-5 col-md-offset-1 col-md-5 col-sm-offset-1 col-sm-5") Close
