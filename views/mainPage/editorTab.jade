div#editorTab(class="formWrapperStyle rowFlexContainer col-lg-12 col-md-12")
  div(class="tabContent active col-lg-12 col-md-12")
    div(class="header rowFlexContainer ow")
      h3(class="col-lg-12 col-md-12") Card Games
    div(class="body row")
      div(class="col-lg-12 col-md-12")
        table#myCardgames(class="col-lg-12 col-md-12")
          thead
            tr
              th(class="col-lg-2") Name
              th(class="col-lg-2") Language
          tbody
    div(class="formBottomStyle bottom row")
      div(class="rowFlexContainer col-lg-12 col-md-12")
        div(class="rowFlexContainer col-lg-5 col-md-5" style="height:100%")
          button#importCardGame(class="myCustomButton col-lg-5 col-md-5") Import
          button#exportCardGame(class="myCustomButton col-lg-offset-1 col-lg-5 col-md-offset-1 col-md-5") Export
        div(class="rowFlexContainer col-lg-offset-2 col-lg-5 col-md-offset-2 col-md-5" style="height:100%")
          button#createCardGame(class="myCustomButton col-lg-5 col-md-5") Create
          button#editCardGame(class="myCustomButton col-lg-offset-1 col-lg-5 col-md-offset-1 col-md-5") Edit

  div(class="importPage col-lg-12 col-md-12")
    div(class="header rowFlexContainer row")
      h3(class="col-lg-12 col-md-12") Import CSV
    div(class="body row")
      div(class="rowFlexContainer row")
        span#importAlertMessage
      div(class="rowFlexContainer row")
        button#importButton(class="myCustomInput col-lg-8 col-md-8") No file chosen
        input#import(type="file" accept=".csv")
    div(class="formBottomStyle bottom row")
      div(class="rowFlexContainer col-lg-12 col-md-12")
        button#cancelImport(class="myCustomButton col-lg-offset-4 col-lg-3 col-md-offset-4 col-md-3") Cancel
        button#valideImport(class="myCustomButton col-lg-offset-1 col-lg-3 col-md-offset-1 col-md-3") Validate
